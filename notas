    if (p2->isBeingHit == 1) {
        *movement = DAMAGED;
        p2->speed_x = p2->direction ? 1 : -1;
        return;
    } else if (p2->isBeingHit == 2) {
        *movement = DAMAGED_DOWN;
        p2->speed_x = p2->direction ? 1 : -1;
        return;
    } else if (p2->isBeingHit == 3) {
        *movement = DAMAGED_JMP;
        p2->speed_x = p2->direction ? 1 : -1;
        return;
    }
